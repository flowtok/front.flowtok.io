{
  "eslintConfig": {
    "extends": ["react-app", "shared-config"],
    "plugins": ["prettier"],
    "rules": {
      "additional-rule": "warn"
    },
    "overrides": [
      {
        "files": ["**/*.ts?(x)"],
        "rules": {
          "additional-typescript-only-rule": "warn",
          "semi": ["error", "always"],
          "prettier/prettier": "error"
        }
      }
    ]
  }
}